{
  "name": "cleanly-server",
  "version": "0.0.1",
  "description": "server side for cleanly app",
  "main": "src/server.ts",
  "scripts": {
    "test": "jest",
    "test:dev": "jest --watchAll",
    "test:cov": "jest --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "start:local": "NODE_ENV=development ts-node .",
    "start": "NODE_ENV=production ts-node .",
    "start:dev": "NODE_ENV=development ts-node-dev .",
    "migrate:local:init": "touch local.sqlite && NODE_ENV=development db-migrate up",
    "migrate:local:up": "NODE_ENV=development db-migrate up",
    "migrate:local:down": "NODE_ENV=development db-migrate down"
  },
  "repository": "git@github.com:cleanlyer/cleanly-server.git",
  "author": "kanekotic <alvarojosepl@gmail.com>",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "apollo-server-express": "2.11.0",
    "datasource-sql": "1.1.1",
    "express": "4.17.1",
    "graphql": "14.6.0",
    "pg": "7.18.2",
    "ts-node": "8.6.2",
    "typescript": "3.8.3",
    "uuid": "7.0.2"
  },
  "devDependencies": {
    "@types/express": "4.17.3",
    "@types/graphql": "14.5.0",
    "@types/jest": "25.1.4",
    "@types/node": "13.9.1",
    "@types/uuid": "7.0.0",
    "db-migrate": "^0.11.6",
    "db-migrate-sqlite3": "0.4.0",
    "faker": "4.1.0",
    "jest": "25.1.0",
    "nodemon": "2.0.2",
    "sqlite3": "4.1.1",
    "ts-node-dev": "1.0.0-pre.44"
  },
  "jest": {
    "testEnvironment": "node",
    "automock": false
  }
}
